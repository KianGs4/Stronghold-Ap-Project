<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="174.0" prefWidth="200" style="-fx-background-color: #b29663; -fx-border-color: black"
            xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="view.GameMenus.SelectedUnitMenus.CoordinatePopupMenu">
    <Label layoutX="-3.0" layoutY="-1.0" prefHeight="60.0" prefWidth="200.0" text="Please Enter Final Coordinate"
           textAlignment="CENTER" wrapText="true">
        <font>
            <Font name="Constantia" size="16.0"/>
        </font>
    </Label>
    <TextField layoutX="14.0" layoutY="59.0" opacity="0.5" prefHeight="30.0" prefWidth="172.0"
               promptText="Please enter x-coordinate" scaleShape="false"
               style="-fx-background-color: #806a3c; -fx-text-fill: #000000; -fx-border-radius: 0; -fx-background-radius: 0;"
               fx:id="xCord"/>
    <ImageView fitHeight="115.0" fitWidth="142.0" layoutX="28.0" layoutY="135.0" onMouseClicked="#back"
               pickOnBounds="true" preserveRatio="true">
        <Image url="@../../../assets/buttons/ok_btn.jpg"/>
    </ImageView>
    <TextField layoutX="14.0" layoutY="96.0" opacity="0.5" prefHeight="30.0" prefWidth="172.0"
               promptText="Please enter y-coordinate" scaleShape="false"
               style="-fx-background-color: #806a3c; -fx-text-fill: #000000; -fx-border-radius: 0; -fx-background-radius: 0;"
               fx:id="yCord"/>
</AnchorPane>
